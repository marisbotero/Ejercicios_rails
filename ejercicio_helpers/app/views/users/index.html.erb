<h1> Fomulario </h1>
<div>
<%=form_tag("/contact", method:"put") do %>
<p><%= label_tag(:nombre, "Nombre") %></p>
<p><%=text_field_tag(:nombre)%></p>
<p><%= label_tag(:apellido, "Apellido") %></p>
<p><%=text_field_tag(:apellido)%></p>
<p><%= label_tag(:email, "Email") %></p>
<p><%=text_field_tag(:email)%></p>
<p><%= label_tag(:fecha, "Fecha de Nacimiento") %></p>
<p><%= select_date Date.today, prefix: :fecha%></p>

<p><%=submit_tag("Enviar") %></p>
</div>
<% end %>

